{
  "name": "react-tdd",
  "version": "1.0.0",
  "description": "- Tutorial on Testing React with TDD     - https://medium.com/tribalscale/tutorial-on-testing-react-part-1-2c587e39114d",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --history-api-fallback --inline --progress --colors --host 0.0.0.0",
    "clean": "if [ -d \"build\" ]; then rm -r build; fi && mkdir build",
    "build": "webpack -p --progress --release",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --require ./tests/test_helper \"tests/**/*_test@(.js|.jsx)\"",
    "test:single":"mocha --compilers js:babel-core/register --require babel-polyfill \"tests/**/math_test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminhuanghuang/react-tdd.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benjaminhuanghuang/react-tdd/issues"
  },
  "homepage": "https://github.com/benjaminhuanghuang/react-tdd#readme",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "redux": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.1.0" 
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.10.0",
    "mocha": "^5.2.0",
    "chai":"^4.1.2",
    "sinon": "^5.0.7",
    "react-addons-test-utils": "^15.6.2",
    "sass-loader": "^7.0.1",
    "unexpected": "^10.37.4",
    "unexpected-react": "^5.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "mini-css-extract-plugin": "^0.4.0"
  }
}
